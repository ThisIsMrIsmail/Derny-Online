{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "manifest-file-extension": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 107",
    "not firefox <= 107"
  ]
}